        </ComboBox>
        <Button Name="cmdAddDataset"
                Width="53"
                Height="23"
                Margin="12,0,0,12"
                HorizontalAlignment="Left"
                VerticalAlignment="Bottom"
                Click="cmdAddDataset_Click"
