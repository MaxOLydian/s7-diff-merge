        <Button Name="cmdRemoveDataset"
                Width="53"
                Height="23"
                Margin="118,0,0,12"
                HorizontalAlignment="Left"
                VerticalAlignment="Bottom"
                Click="cmdRemoveDataset_Click"
                Content="Remove" />
        <Rectangle Name="rectangle2"
                   Width="2"
                   Margin="176,5,0,5"
                   HorizontalAlignment="Left"
                   Fill="Black"
                   Stroke="Black" />
        <Button Name="cmdCopyDataset"
                Width="41"
